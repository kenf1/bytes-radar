{
  "name": "bytes-radar-wasm",
  "version": "0.2.0",
  "description": "WebAssembly build of bytes-radar - A fast code analysis tool for remote repositories",
  "main": "dist/node/index.js",
  "module": "dist/web/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:web && npm run build:node && npm run build:types",
    "build:web": "wasm-pack build --target web --out-dir dist/web --features wasm ..",
    "build:node": "wasm-pack build --target nodejs --out-dir dist/node --features wasm ..",
    "build:types": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "code-analysis",
    "git",
    "repository",
    "statistics"
  ],
  "author": "ProgramZmh <zmh@lightxi.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmh-program/bytes-radar.git"
  },
  "devDependencies": {
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "@wasmer/wasi": "^1.2.2",
    "@wasmer/wasmfs": "^0.12.0"
  }
} 